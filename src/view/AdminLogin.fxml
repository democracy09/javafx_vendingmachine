<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Button?>
<?import javafx.scene.control.Label?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.layout.AnchorPane?>

<AnchorPane fx:id="mainpane" prefHeight="200.0" prefWidth="500.0" stylesheets="@mainstylesheet.css" xmlns="http://javafx.com/javafx/11.0.1" xmlns:fx="http://javafx.com/fxml/1" fx:controller="view.LoginController">
   <children>
      <AnchorPane prefHeight="200.0" prefWidth="491.0" styleClass="container" stylesheets="@mainstylesheet.css" AnchorPane.bottomAnchor="14.0" AnchorPane.leftAnchor="14.0" AnchorPane.rightAnchor="14.0" AnchorPane.topAnchor="14.0">
         <children>
            <Button fx:id="mainBtn" layoutX="18.0" layoutY="13.0" mnemonicParsing="false" onAction="#gotoMain" styleClass="btn" text="메인으로" />
            <Label layoutX="93.0" layoutY="53.0" text="관리자메뉴 로그인" />
         </children>
      </AnchorPane>
      <TextField fx:id="txtPassword" layoutX="106.0" layoutY="89.0" prefHeight="22.0" prefWidth="219.0" />
      <Button fx:id="loginBtn" layoutX="343.0" layoutY="89.0" mnemonicParsing="false" onAction="#pressLogin" styleClass="btn" text="Login" />
      <Button fx:id="setPassword" layoutX="306.0" layoutY="124.0" mnemonicParsing="false" onAction="#goSetPw" styleClass="btn" text="비밀번호 설정" />
   </children>
</AnchorPane>
